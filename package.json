        "sass-loader": "^7.1.0",
        "tailwindcss": "^3.2.4",
        "vue": "^2.6.12",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@inertiajs/inertia": "^0.11.1",
